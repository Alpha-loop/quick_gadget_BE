{
  "version": 2,
  "functions": {
    "routes/**/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/health",
      "dest": "/server.js"
    }
  ]
}